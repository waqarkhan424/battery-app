{
  "expo": {
    "name": "battery-app",
    "slug": "battery-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "batteryapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.batteryapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      "expo-splash-screen",

      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "37c7f23e-a85f-4617-a1a9-d04a53be7300"
      }
    },
    "owner": "waqar-424"
  }
}
