{
  "expo": {
    "name": "battery-app",
    "slug": "battery-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "batteryapp",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/images/launcher-icon.png",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.batteryapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/launcher-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      "expo-splash-screen",
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "37c7f23e-a85f-4617-a1a9-d04a53be7300"
      }
    },
    "owner": "waqar-424"
  }
}
