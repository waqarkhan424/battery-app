{
  "expo": {
    "name": "Charging Animation",
    "slug": "battery-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "batteryapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.batteryapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      "expo-splash-screen",
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "enableMinifyInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9ee2afe7-d1d3-4b53-96cf-8eda96481974"
      }
    },
    "owner": "waqar424"
  }
}
